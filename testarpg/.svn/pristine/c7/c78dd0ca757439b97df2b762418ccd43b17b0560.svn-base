
module egret {

	export class SceneElementEvent extends Event{
		/**
		 * 场景元素移动，不冒泡 
		 */		
		public static SCENE_ELEMENT_MOVING:string = "sceneElementMoving";
		/**
		 * 场景元素移动结束，冒泡 
		 */		
		public static SCENE_ELEMENT_MOVING_END:string = "sceneElementMovingEnd";
		/**
		 * 场景元素路径改变，不冒泡 
		 */		
		public static SCENE_ELEMENT_PATH_CHANGED:string = "sceneElementPathChanged";
		/**
		 * 构造函数
		 */
		public constructor(type:string, bubbles:boolean=false, cancelable:boolean=false){
			super(type, bubbles, cancelable);
		}
	}
}