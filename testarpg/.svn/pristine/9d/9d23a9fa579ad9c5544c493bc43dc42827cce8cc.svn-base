
module egret {

	export class SceneEditLo{
		/**
		 * 主键 
		 */		
		public id:number = 0;
		/**
		 * 切片宽度
		 */		
		public pieceWidth:number = 0;
		/**
		 * 切片高度
		 */		
		public pieceHeight:number = 0;
		/**
		 * 路径方格像素大小(高度值，宽度为此值2倍) 
		 */		
		public gridSize:number = 0;
		/**
		 * 地图总宽度 
		 */		
		public width:number = 0;
		/**
		 * 地图总高度 
		 */		
		public height:number = 0;
		/**
		 * 总列数 
		 */		
		public columns:number = 0;
		/**
		 * 总行数 
		 */		
		public rows:number = 0;
		/**
		 * 节点数组，2维数组，1行为1个数组 
		 */		
		public nodeTypes:Array<any> = null;
		/**
		 * 地图中的元素数据集合，以SceneElementType为key，元素数据(SceneElementLo)数组为值的集合 
		 */		
		public elements:any = null;
	}
}